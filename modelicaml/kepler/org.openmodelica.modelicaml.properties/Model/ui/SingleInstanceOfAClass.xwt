<?xml version="1.0" encoding="UTF-8"?>
<Composite xmlns:ppe="clr-namespace:org.eclipse.papyrus.views.properties.widgets"
	xmlns:j="clr-namespace:java.lang" xmlns:ppel="clr-namespace:org.eclipse.papyrus.views.properties.widgets.layout"
	xmlns:x="http://www.eclipse.org/xwt" xmlns="http://www.eclipse.org/xwt/presentation">
	<Composite.layout>
		<ppel:PropertiesLayout></ppel:PropertiesLayout>
	</Composite.layout>
	<Composite>
		<Composite.layout>
			<ppel:PropertiesLayout numColumns="1"></ppel:PropertiesLayout>
		</Composite.layout>
		<ppe:StringEditor input="{Binding}"
			property="ModelicaML:ModelicaCompositeConstructs:InstanceOfAClass:conditionalExpression"></ppe:StringEditor>
		<ppe:StringEditor input="{Binding}"
			property="ModelicaML:ModelicaCompositeConstructs:InstanceOfAClass:arraySize"></ppe:StringEditor>
		<ppe:StringEditor input="{Binding}"
			property="ModelicaML:ModelicaCompositeConstructs:InstanceOfAClass:modification"></ppe:StringEditor>
		<ppe:StringEditor input="{Binding}"
			property="ModelicaML:ModelicaCompositeConstructs:InstanceOfAClass:iconImagePath"></ppe:StringEditor>
	</Composite>
	<Composite>
		<Composite.layout>
			<ppel:PropertiesLayout numColumns="2"></ppel:PropertiesLayout>
		</Composite.layout>
		<ppe:BooleanRadio input="{Binding}"
			property="ModelicaML:ModelicaCompositeConstructs:InstanceOfAClass:replaceable"></ppe:BooleanRadio>
		<ppe:BooleanRadio input="{Binding}"
			property="ModelicaML:ModelicaCompositeConstructs:InstanceOfAClass:final"></ppe:BooleanRadio>
	</Composite>
</Composite>