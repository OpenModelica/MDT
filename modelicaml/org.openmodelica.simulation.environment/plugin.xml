<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.openmodelica.simulation.environment.wizard.category.newsimulationprojectwizard"
            name="Modelica Simulation">
      </category>
      <wizard
            category="org.openmodelica.simulation.environment.wizard.category.newsimulationprojectwizard"
            class="org.openmodelica.simulation.environment.wizard.newsimulationproject.NewSimulationProjectFromExistingCodeWizard"
            icon="icons/ModelicaSimulationPerspective.gif"
            id="org.openmodelica.simulation.environment.wizard.newsimulationproject.fromexistingcodewizard"
            name="Simulation Project from existing code">
      </wizard>
      <wizard
            category="org.openmodelica.simulation.environment.wizard.category.newsimulationprojectwizard"
            class="org.openmodelica.simulation.environment.wizard.newsimulationproject.NewSimulationProjectFromExistingRuntimeWizard"
            icon="icons/ModelicaSimulationPerspective.gif"
            id="org.openmodelica.simulation.environment.wizard.newsimulationproject.fromexistingruntimewizard"
            name="Simulation Project from existing runtime">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            name="Modelica Simulation View">
      </category>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.project.SimulationProjectDataView"
            id="org.openmodelica.simulation.environment.view.project.simulationprojectdata"
            name="Simulation Project Data"
            restorable="true">
      </view>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.project.SimulationProjectListingExtendedView"
            id="org.openmodelica.simulation.environment.view.project.simulationprojectlistingextended"
            name="Simulation Project Listing 2"
            restorable="true">
      </view>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.simulation.interactive.InteractiveSimulationSessionControlView"
            id="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationsessioncontrol"
            name="Interactive Simulation Session Control"
            restorable="true">
      </view>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.simulation.interactive.InteractiveSimulationPropertyControlView"
            id="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationpropertycontrol"
            name="Interactive Simulation Property Control"
            restorable="true">
      </view>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.simulation.interactive.InteractiveSimulationVisualisationView"
            id="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationvisualisationview"
            name="Interactive Simulation Visualisation"
            restorable="true">
      </view>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.simulation.noninteractive.NonInteractiveSimulationVisualisationView"
            id="org.openmodelica.simulation.environment.view.simulation.noninteractive.simulationvisualisationview"
            name="Non-Interactive Simulation Visualisation"
            restorable="true">
      </view>
      <view
            category="org.openmodelica.simulation.environment.view.category.modelicaSimulationView"
            class="org.openmodelica.simulation.environment.view.simulation.noninteractive.NonInteractiveSimulationControlView"
            id="org.openmodelica.simulation.environment.simulation.noninteractive.simulationcontrolview"
            name="Non-Interactive Simulation Control"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.openmodelica.simulation.environment.perspective.ModelicaSimulationProjectPerspective"
            icon="icons/ModelicaSimulationPerspective.gif"
            id="org.openmodelica.simulation.environment.perspective.project.ProjectDataPerspective"
            name="Modelica Simulation Project">
      </perspective>
      <perspective
            class="org.openmodelica.simulation.environment.perspective.ModelicaInteractiveSimulationSessionPerspective"
            icon="icons/ModelicaSimulationPerspective.gif"
            id="org.openmodelica.simulation.environment.perspective.simulation.SimulationSessionPerspective"
            name="Modelica Interactive Simulation Session">
      </perspective>
      <perspective
            class="org.openmodelica.simulation.environment.perspective.ModelicaNonInteractiveSimulationSessionPerspective"
            icon="icons/ModelicaSimulationPerspective.gif"
            id="org.openmodelica.simulation.environment.perspective.simulation.noninteractiveSimulationSessionPerspective"
            name="Modelica Non-Interactive Simulation">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.openmodelica.simulation.environment.perspective.project.ProjectDataPerspective">
         <view
               closeable="false"
               id="org.openmodelica.simulation.environment.view.project.simulationprojectlistingextended"
               minimized="false"
               moveable="true"
               ratio="0.3f"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.openmodelica.simulation.environment.view.project.simulationprojectdata"
               minimized="false"
               moveable="true"
               ratio="0.6f"
               relationship="right"
               relative="org.openmodelica.simulation.environment.view.project.simulationprojectlistingextended"
               showTitle="false"
               standalone="false"
               visible="true">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.openmodelica.simulation.environment.perspective.simulation.SimulationSessionPerspective">
         <view
               closeable="true"
               id="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationsessioncontrol"
               minimized="false"
               moveable="true"
               ratio="0.3f"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationpropertycontrol"
               minimized="false"
               moveable="true"
               ratio="0.6f"
               relationship="bottom"
               relative="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationsessioncontrol"
               showTitle="false"
               standalone="false"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.openmodelica.simulation.environment.view.simulation.interactive.interactivesimulationvisualisationview"
               minimized="false"
               moveable="true"
               ratio="0.6f"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               showTitle="false"
               standalone="false"
               visible="true">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.openmodelica.simulation.environment.perspective.simulation.noninteractiveSimulationSessionPerspective">
         <view
               closeable="true"
               id="org.openmodelica.simulation.environment.simulation.noninteractive.simulationcontrolview"
               minimized="false"
               moveable="true"
               ratio="0.3f"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.openmodelica.simulation.environment.view.simulation.noninteractive.simulationvisualisationview"
               minimized="false"
               moveable="true"
               ratio="0.6f"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               showTitle="false"
               standalone="false"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.openmodelica.simulation.environment.preferences.page.WorkbenchPreferencePageDefaultConfiguration"
            id="org.openmodelica.simulation.environment.preferences.page.page1"
            name="OpenModelica Interactive">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.openmodelica.simulation.environment.AbstractPreferenceInitializerForOMI">
      </initializer>
   </extension>

</plugin>
