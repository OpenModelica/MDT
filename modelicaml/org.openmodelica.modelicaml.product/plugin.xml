<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="ModelicaML">
         <property
               name="appName"
               value="ModelicaML">
         </property>
         <property
               name="aboutImage"
               value="platform:/plugin/org.openmodelica.modelicaml.common/icons/logos/ModelicaML_logo_64x64.png">
         </property>
         <property
               name="aboutText"
               value="ModelicaML is a UML Profile for modeling and simulating of the system dynamic behavior. It enables engineers to formalize system requirements and designs, to simulate designs  and to verify desings againts requirements.">
         </property>
         <property
               name="windowImages"
               value="platform:/plugin/org.openmodelica.modelicaml.common/icons/logos/ModelicaML_logo_16x16.png,platform:/plugin/org.openmodelica.modelicaml.common/icons/logos/ModelicaML_logo_32x32.png,platform:/plugin/org.openmodelica.modelicaml.common/icons/logos/ModelicaML_logo_48x48.png,platform:/plugin/org.openmodelica.modelicaml.common/icons/logos/ModelicaML_logo_64x64.png,platform:/plugin/org.openmodelica.modelicaml.common/icons/logos/ModelicaML_logo_128x128.png">
         </property>
      </product>
   </extension>

</plugin>
